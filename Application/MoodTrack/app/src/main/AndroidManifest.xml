<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rever.moodtrack">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MoodTrack">
        <activity android:name=".About" android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".Settings" android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".Statistics"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".DayQuestions"
            android:parentActivityName=".DayMood" />
        <activity
            android:name=".DayMood"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>